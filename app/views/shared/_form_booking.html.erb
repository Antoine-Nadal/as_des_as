<div class="container mt-3">
  <h1>Réservez un bar à jeu:</h1>
  <%= simple_form_for [@gaming_session, @booking] do |f| %>
    <%# Prénom: <%= @gaming_session.user.first_name %>
    <%# Nom: <%= @gaming_session.user.last_name %
    <div class="input">
      <%= f.input :start_date, :input_html => {class: 'response-project'}%>
      <%= f.input :end_date, :input_html => {class: 'response-project'}%>
    </div>
    <div class="submit">
      <%= f.button :submit 'réserver' class: "button-orange" %>
    </div>
  <% end %>
</div>
