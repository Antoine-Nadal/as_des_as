<div class="container">
  <%= simple_form_for(gaming_session) do |f| %>
  <div class="input">
      <%= f.input :name, :input_html => {class: 'response-project'}%>
      <%= f.input :date, :input_html => {class: 'response-project'}%>
      <%= f.input :address, :input_html => {class: 'response-project'}%>
      <%= f.input :game, :input_html => {class: 'response-project'}%>
      <%= f.input :number_of_participants, :input_html => {class: 'response-project'}%>
      <%= f.input :description, :input_html => {class: 'response-project'}%>
      <%= f.input :price, :input_html => {class: 'response-project'}%>
      <%= f.input :photos, as: :file, input_html: { multiple: true } , :input_html => {class: 'response-project'}%>
      <div class="submit">
        <%= f.button :submit, class: "button-orange" %>
      </div>
    </div>
  <% end %>
</div>
